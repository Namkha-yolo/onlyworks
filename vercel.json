{
  "buildCommand": "",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/roast.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/register",
      "destination": "/register.html"
    },
    {
      "source": "/employer",
      "destination": "/employer.html"
    }
  ]
}
